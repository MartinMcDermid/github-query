{
  "name": "github-commit-titles",
  "version": "1.0.2",
  "description": "Fetch commit titles from GitHub repositories within a date range",
  "main": "titles.js",
  "bin": {
    "github-commits": "./titles.js"
  },
  "scripts": {
    "start": "node titles.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:verbose": "jest --verbose",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:workflows": "actionlint .github/workflows/*.yml",
    "lint:yaml": "yamllint .github/workflows/*.yml",
    "lint:all": "npm run lint && npm run lint:workflows && npm run lint:yaml",
    "release": "release-it",
    "release:patch": "release-it patch",
    "release:minor": "release-it minor",
    "release:major": "release-it major",
    "release:prerelease": "release-it prerelease",
    "release:dry": "release-it --dry-run"
  },
  "keywords": [
    "github",
    "commits",
    "api",
    "cli",
    "git",
    "history"
  ],
  "author": "Martin McDermid",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "yaml": "^2.3.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MartinMcDermid/github-query.git"
  },
  "bugs": {
    "url": "https://github.com/MartinMcDermid/github-query/issues"
  },
  "homepage": "https://github.com/MartinMcDermid/github-query#readme",
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@release-it/conventional-changelog": "^10.0.1",
    "eslint": "^9.35.0",
    "eslint-plugin-jest": "^29.0.1",
    "jest": "^30.1.3",
    "release-it": "^19.0.4"
  }
}
